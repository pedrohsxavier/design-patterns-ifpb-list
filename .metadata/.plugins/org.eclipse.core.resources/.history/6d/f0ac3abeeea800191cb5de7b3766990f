package view;

import model.Arquivo;
import model.Pasta;

public class Cliente {

	public static void main(String[] args) {

		Arquivo a1 = new Arquivo("Java Tips", 266);
		Arquivo a2 = new Arquivo("Python Tips", 741);
		
		Pasta p1 = new Pasta("Dev Tips");
		
		p1.add(a1);
		p1.add(a2);
		
		p1.toString();
	}

}
