package gof;

import java.util.ArrayList;

public class Cliente {

	public static void main(String[] args) {
		
		MapAdapter m1 = new MapAdapter();
		ArrayList<Integer> k = new ArrayList<>();
		ArrayList<Integer> v = new ArrayList<>();
		k.add(10);
		k.add(21);
		v.add(11);
		k.add(22);
		m1.put(k, v);
		
		System.out.println(m1);
	}

}
